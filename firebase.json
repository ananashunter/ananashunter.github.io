{
  "hosting": {
    "public": "/",
    "trailingSlash": false,
    "cleanUrls": false,
    "ignore": [
      "firebase.json",
      "polymer.json",
      "CNAME",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/doc/*",
        "destination": "/src/doc/*"
      }, {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "redirects": [
      {
        "source": "/index.html",
        "destination": "/",
        "type" : 301
      },
      {
        "source": "/Animation-Reel.html",
        "destination": "/animation",
        "type" : 301
      },
      {
        "source": "/story.html",
        "destination": "/story",
        "type" : 301
      },
      {
        "source": "/comic.html",
        "destination": "/comic",
        "type" : 301
      },
      {
        "source": "/Illustrations-Doodles.html",
        "destination": "/illustrations",
        "type" : 301
      },
      {
        "source": "/Sketches.html",
        "destination": "/sketches",
        "type" : 301
      },
      {
        "source": "/8.html",
        "destination": "/studies",
        "type" : 301
      },
      {
        "source": "/Info-resume-contact.html",
        "destination": "/about",
        "type" : 301
      }
    ],
    "headers": []
  }
}
